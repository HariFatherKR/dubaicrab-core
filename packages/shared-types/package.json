{
  "name": "@dubaicrab/shared-types",
  "version": "0.1.0",
  "description": "Dubai Crab 공유 타입 정의",
  "keywords": [
    "dubaicrab",
    "types",
    "typescript"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/snovium/dubaicrab-core.git",
    "directory": "packages/shared-types"
  },
  "files": [
    "dist",
    "src"
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./chat": {
      "types": "./dist/chat.d.ts",
      "import": "./dist/chat.js"
    },
    "./gateway": {
      "types": "./dist/gateway.d.ts",
      "import": "./dist/gateway.js"
    },
    "./file": {
      "types": "./dist/file.d.ts",
      "import": "./dist/file.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc && npm run build:cjs",
    "build:cjs": "tsc --module commonjs --outDir dist --declaration false && for f in dist/*.js; do mv \"$f\" \"${f%.js}.cjs\"; done 2>/dev/null || true",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build",
    "version:check": "echo \"@dubaicrab/shared-types@$npm_package_version\""
  },
  "devDependencies": {
    "typescript": "^5.0.0"
  },
  "peerDependencies": {},
  "engines": {
    "node": ">=18.0.0"
  }
}
